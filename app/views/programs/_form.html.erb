<%= simple_form_for(@program) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :prog_name %>
    <%= f.input :prog_obj %>
    <%= f.input :organization_id, collection: Organization.all, label_method: :name %>
  </div>




  <div class="form-actions">
    <%= f.button :submit %>
  </div>


<% end %>
