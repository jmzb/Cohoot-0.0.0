
<div class="container-fluid bkgnd1">	
	<div class="nav-pills pull-right row-head2">
		<%= link_to "Sign In", new_user_session_path, :class => "btn btn-primary btn-lg", :type => "button"%>	
	</div>
<div class="container-fluid">
<div class="row-head2">	
	<div class="col-md-8 col-md-offset-2 row-head">
		<%= image_tag("cohoot_logo2.png", :class => "img-responsive", :alt => "cohoot?") %>
	</div>
  	<div class="col-md-4 col-md-offset-4 row-bottom">
		<div class="panel-trans panel-default">
  	  		<div class="panel-body"> 

	<%= simple_form_for(resource, 
		:as => resource_name, 
		:url => registration_path(resource_name),
		:html => { :class => 'form-horizontal container-fluid col-md-8 col-md-offset-2' },
		:wrapper => :bootstrap3) do |f| %>
  	<%= f.error_notification %> </br>	
  	<%= f.input_field :first_name, placeholder: 'first name', :required => true, :autofocus => true %> </br>
  	<%= f.input_field :last_name, placeholder: 'last name', :required => true, :autofocus => true %> </br>	
    <%= f.input_field :email, placeholder: 'email', :required => true %> </br>	
    <%= f.input_field :password, placeholder: 'password', :required => true %> </br>	  
    <%= f.input_field :password_confirmation, placeholder: 'confirm password', :required => true %> </br>
    <%= f.button :submit, 'Sign up', :class => "btn btn-primary btn-lg" %>
	<% end %>

  			</div> <!--end panel-body-->
		</div> <!--end panel-->
	</div> <!--end col-md-4-->
</div> <!--end row-->	
</div> <!--end container-fluid -->

</div> <!--end container-fluid bkgnd1-->




